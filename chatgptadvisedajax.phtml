\n        \u003cselect id=\"suffix\"\u003e\n            \u003coption value=\"mxuni\"\u003emxuni\u003c/option\u003e\n            \u003coption value=\"wsldebian\"\u003ewsldebian\u003c/option\u003e\n            \u003coption value=\"mxlinux-21\"\u003emxlinux-21\u003c/option\u003e\n        \u003c/select\u003e\n        \u003c!-- New select dropdown for choosing innerHTML values --\u003e\n        \u003cselect id=\"listItemValue\"\u003e\n            \u003c!-- Dynamically populated options will be inserted here --\u003e\n        \u003c/select\u003e\n        \u003cbutton type=\"button\" onclick=\"appendSelectedValue()\"\u003eAppend Selected Value\u003c/button\u003e\n        \u003cbutton type=\"button\" onclick=\"updateAppendedData()\"\u003eUpdate Appended Data\u003c/button\u003e\n    \u003c/form\u003e\n    \u003cdiv id=\"appendedData\"\u003e\u003ca\u003e\u003c/a\u003e\u003c/div\u003e\n\n    \u003cscript\u003e\n        // Function to populate the select dropdown with innerHTML values\n        function populateListItemValues() {\n            const listContainer = document.getElementById('listContainer');\n            const listItemElements = listContainer.querySelectorAll('li');\n            const listItemValueSelect = document.getElementById('listItemValue');\n\n            // Clear existing options\n            listItemValueSelect.innerHTML = '';\n\n            // Create and append options for each list item\n            listItemElements.forEach(function(listItemElement) {\n                const value = listItemElement.textContent.trim().split(':')[1].trim();\n                const option = document.createElement('option');\n                option.value = value;\n                option.textContent = value;\n                listItemValueSelect.appendChild(option);\n            });\n        }\n\n        // Function to append the selected value to the input field\n        function appendSelectedValue() {\n            const listItemValueSelect = document.getElementById('listItemV